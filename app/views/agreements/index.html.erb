<h1>Listing agreements</h1>

<table>
  <thead>
    <tr>
      <th>Campaign</th>
      <th>Organization</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @agreements.each do |agreement| %>
      <tr>
        <td><%= agreement.phase_id %></td>
        <td><%= agreement.group_id %></td>
        <td><%= link_to 'Show', agreement %></td>
        <td><%= link_to 'Edit', edit_agreement_path(agreement) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

